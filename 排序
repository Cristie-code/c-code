#define _CRT_SECURE_NO_WARNINGS_
#include<stdio.h>
int main(void)
{
	int a = 0;
	int b = 0;
	int c = 0;
	scanf_s("%d%d%d", &a, &b, &c);
	if (a < b)
	{
		int ex = a;
		a = b;
		b = ex;
	}
	if (b < c)
	{
		int xp = b;
		b = c;
		c = xp;
	}
	if (a < c)
	{
		int pe = a;
		a = c;
		c = pe;
	}
	printf("%d %d %d",a,b,c);
	return 0;
}
